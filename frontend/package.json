{
  "name": "rare-basket-frontend",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --no-progress",
    "test": "ng test --code-coverage --no-progress --no-watch",
    "lint": "pnpx mkdirp build && ng lint > build/eslint-result.txt",
    "format": "prettier --write \"{src,e2e}/**/*.+(ts|js|json|html|scss)\"",
    "e2e": "pnpm playwright test",
    "postinstall": "pnpm exec playwright install",
    "knip": "pnpx knip --fix-type exports,types --exclude dependencies,unlisted,files,binaries --tags=knipignore"
  },
  "private": true,
  "packageManager": "pnpm@10.13.1",
  "engines": {
    "node": "22.17.0"
  },
  "dependencies": {
    "@angular/common": "20.1.0",
    "@angular/compiler": "20.1.0",
    "@angular/core": "20.1.0",
    "@angular/forms": "20.1.0",
    "@angular/localize": "20.1.0",
    "@angular/platform-browser": "20.1.0",
    "@angular/router": "20.1.0",
    "@fortawesome/angular-fontawesome": "2.0.1",
    "@fortawesome/free-solid-svg-icons": "6.7.2",
    "@ng-bootstrap/ng-bootstrap": "19.0.1",
    "@ngx-translate/core": "16.0.4",
    "@popperjs/core": "2.11.8",
    "angular-auth-oidc-client": "19.0.1",
    "bootstrap": "5.3.7",
    "chart.js": "4.5.0",
    "ngx-valdemort": "14.0.0",
    "papaparse": "5.5.3",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "20.1.0",
    "@angular/cli": "20.1.0",
    "@angular/compiler-cli": "20.1.0",
    "@eslint/js": "9.30.1",
    "@playwright/test": "1.54.0",
    "@types/jasmine": "5.1.8",
    "@types/papaparse": "5.3.16",
    "angular-eslint": "20.1.1",
    "eslint": "9.30.1",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-prettier": "5.5.1",
    "jasmine-core": "5.8.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "ngx-speculoos": "14.0.0",
    "prettier": "3.6.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.36.0"
  }
}
