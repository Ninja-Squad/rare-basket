{
  "name": "rare-basket-frontend",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --no-progress",
    "test": "ng test --code-coverage --no-progress --no-watch",
    "lint": "pnpx mkdirp build && ng lint > build/eslint-result.txt",
    "format": "prettier --write \"{src,e2e}/**/*.+(ts|js|json|html|scss)\"",
    "e2e": "pnpm playwright test",
    "postinstall": "pnpm exec playwright install",
    "knip": "pnpx knip --fix-type exports,types --exclude dependencies,unlisted,files,binaries --tags=knipignore"
  },
  "private": true,
  "packageManager": "pnpm@10.8.1",
  "engines": {
    "node": "22.14.0"
  },
  "dependencies": {
    "@angular/common": "19.2.7",
    "@angular/compiler": "19.2.7",
    "@angular/core": "19.2.7",
    "@angular/forms": "19.2.7",
    "@angular/localize": "19.2.7",
    "@angular/platform-browser": "19.2.7",
    "@angular/platform-browser-dynamic": "19.2.7",
    "@angular/router": "19.2.7",
    "@fortawesome/angular-fontawesome": "1.0.0",
    "@fortawesome/free-solid-svg-icons": "6.7.2",
    "@ng-bootstrap/ng-bootstrap": "18.0.0",
    "@ngx-translate/core": "16.0.4",
    "@popperjs/core": "2.11.8",
    "angular-auth-oidc-client": "19.0.0",
    "bootstrap": "5.3.5",
    "chart.js": "4.4.9",
    "ngx-valdemort": "13.0.0",
    "papaparse": "5.5.2",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.2.8",
    "@angular/cli": "19.2.8",
    "@angular/compiler-cli": "19.2.7",
    "@eslint/js": "9.25.0",
    "@playwright/test": "1.52.0",
    "@types/jasmine": "5.1.7",
    "@types/papaparse": "5.3.15",
    "angular-eslint": "19.3.0",
    "eslint": "9.25.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-prettier": "5.2.6",
    "jasmine-core": "5.6.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "ngx-speculoos": "13.1.0",
    "prettier": "3.5.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.30.1"
  }
}
